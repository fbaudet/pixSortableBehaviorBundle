{
    "name": "fbaudet/pixassociates_sortable-behavior-bundle",
    "type": "symfony-bundle",
    "description": "Provides a way to sort your admin listing",
    "keywords": ["sortable","admin","sonata"],
    "homepage": "https://github.com/fbaudet/pixSortableBehaviorBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Ricci",
            "email": "nr@pix-digital.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/framework-bundle": ">=5.4",
        "symfony/form": ">=5.4"
    },
    "require-dev": {
        "gedmo/doctrine-extensions": ">=2.2"
    },
    "suggest": {
        "sonata-project/admin-bundle": "Can be easily integrated with Sonata Admin bundle",
        "stof/doctrine-extensions-bundle": "1.1.*@dev"
    },
    "autoload": {
        "psr-0": { "Pix\\SortableBehaviorBundle": "" }
    },
    "target-dir": "Pix/SortableBehaviorBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    }
}
